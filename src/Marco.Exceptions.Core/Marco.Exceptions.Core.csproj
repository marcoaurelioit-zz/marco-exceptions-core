<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Marco Aurélio</Authors>  
    <AssemblyName>Marco.Exceptions.Core</AssemblyName>
    <RootNamespace>Marco.Exceptions.Core</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>   
    <PackageProjectUrl>https://github.com/marcoaurelioit/marco-exceptions-core</PackageProjectUrl>
    <PackageId>Marco.Exceptions.Core</PackageId>  
    <RepositoryType>Public</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.2.2</Version>
  </PropertyGroup>

</Project>
